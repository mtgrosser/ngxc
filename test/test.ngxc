load 'macros/*.ngxconf'

user 'nginx'
worker_processes 'auto'

error_log '/var/logs/nginx/error.log'
pid	      '/run/nginx.pid'

events {
  worker_connections 1024
}

http {
  include      '/etc/nginx/mime.types'
  default_type 'application/octet-stream'
    
  server {
    ssl_encryption 'example.com'
  }
}
